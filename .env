# Copy this file to .env and fill in your values

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_actual_private_key_without_0x

# Snowtrace API key for contract verification
SNOWTRACE_API_KEY=your_actual_snowtrace_api_key

# RPC URLs (optional, defaults are provided in hardhat.config.js)
FUJI_RPC_URL=https://api.avax-test.network/ext/bc/C/rpc
MAINNET_RPC_URL=https://api.avax.network/ext/bc/C/rpc